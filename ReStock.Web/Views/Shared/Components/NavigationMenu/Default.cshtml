@model IEnumerable<string>
<a class="btn btn-block btn-outline-secondary" asp-action="RecipeListDetail"
   asp-controller="Recipe" asp-route-category="">
    Home
</a>
@foreach(string category in Model)
{
    <a class="btn btn-block btn-outline-secondary"
       asp-action="RecipeListDetail" asp-controller="Recipe"
       asp-route-category="@category"
       asp-route-recipePage="1">
        @category
    </a>
}